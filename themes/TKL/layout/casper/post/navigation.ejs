<nav class="pagination" role="pagination">
    <% if (page.prev) { %>
        <a class="pull-left" href="<%- url_for(page.prev.path) %>" style="float: left;">上一篇: <%- page.prev.title %></a>
    <% } %>
    <% if (page.next) { %>
        <a class="pull-right" href="<%- url_for(page.next.path) %>">下一篇: <%- page.next.title %></a>
    <% } %>
</nav>
